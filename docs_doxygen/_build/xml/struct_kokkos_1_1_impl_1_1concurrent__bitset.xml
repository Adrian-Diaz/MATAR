        <argsstring>(uint32_t volatile *const buffer, uint32_t const bit_bound_lg2, uint32_t bit=0, uint32_t const state_header=0) noexcept</argsstring>
        <name>acquire_bounded_lg2</name>
        <qualifiedname>Kokkos::Impl::concurrent_bitset::acquire_bounded_lg2</qualifiedname>
        <param>
          <type>uint32_t volatile *const</type>
          <declname>buffer</declname>
        </param>
        <param>
          <type>uint32_t const</type>
          <declname>bit_bound_lg2</declname>
        </param>
        <param>
